function err = fmr(t, imp, is_sorted, output)
    if output
        tic
    end
    if ~is_sorted
        imp = sortcols(imp,1);
    end
    imp(imp > t)
    %set of all impostor scores s > t
    err = 0;
    for i = 1:size(imp,2)
        if imp(i) > t
            err = (size(imp,2) - i) / size(imp,2);
            break
        end
    end
    if output
        fprintf('t and fmr: %i %i %i %i\n', t, err, size(omgt,2), size(imp_sorted,2))
        toc
    end
end