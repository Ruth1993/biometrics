function errors = perf_fmr(t_min, t_max, imp, output)
    figure()

    num = abs(t_max - t_min) + 1;
    diff_1 = 1 - t_min;
    
    if isempty(imp)
      [~, ~, imp] = get_gen_imp2;  
    end
    
    imp_sorted = sort(imp);
    
    errors = zeros(1, num);
    for t=t_min:1:t_max
        errors(t + diff_1) = fmr(t, imp_sorted, true, output);    
    end
    plot(t_min:t_max, errors);
        title(sprintf("False Match Rate over t = %i to %i", t_min, t_max))
end